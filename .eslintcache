}\n\nexport default class Home extends React.Component {\n  state = {\n    confirmed: 0,\n    recovered: 0,\n    deaths: 0,\n    lastUpdate: \"\",\n  };\n\n  componentDidMount() {\n    this.getData();\n  }\n\n  async getData() {\n    const resApi = await Axios.get(\"https://covid19.mathdro.id/api/\");\n    this.setState({\n      confirmed: resApi.data.confirmed.value,\n      recovered: resApi.data.recovered.value,\n      deaths: resApi.data.deaths.value,\n      lastUpdate: resApi.data.lastUpdate,\n    });\n  }\n\n  thousandSeparator(value) {\n    return value\n      .toFixed(0)\n      .toString()\n      .replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n  }\n\n  render() {\n    return (\n      <Container className=\"home\">\n        <AlertDismissible />\n        <h2 className=\"title\">üåçGLOBAL.</h2>\n        <Row className=\"flex\">\n          <Col md={4} className=\"box confirmed\">\n            <h3>ü§íPositive.</h3>\n            <h4>\n              {\" \"}\n              <AnimatedNumber\n                value={this.state.confirmed}\n                formatValue={this.thousandSeparator}\n              />{\" \"}\n            </h4>\n          </Col>\n          <Col md={4} className=\"box recovered\">\n            <h3>üíâRecovered.</h3>\n            <h4>\n              {\" \"}\n              <AnimatedNumber\n                value={this.state.recovered}\n                formatValue={this.thousandSeparator}\n              />{\" \"}\n            </h4>\n          </Col>\n          <Col md={4} className=\"box deaths\">\n            <h3>‚ò†Ô∏èDeath.</h3>\n            <h4>\n              {\" \"}\n              <AnimatedNumber\n                value={this.state.deaths}\n                formatValue={this.thousandSeparator}\n              />{\" \"}\n            </h4>\n          </Col>\n        </Row>\n        <div style={{ marginBottom: \"15%\" }}>\n          <h4>\n            Last Update:{\" \"}\n            <Moment\n              date={this.state.lastUpdate}\n              format=\"DD MMM YYYY hh:mm\"\n            ></Moment>\n          </h4>\n        </div>\n        <Row className=\"flex\">\n          <Col md={6} className=\"batas\">\n            <Link to=\"/Global\">\n              <Button\n                className=\"tombol\"\n                style={{ width: \"70%\", height: \"150px\", fontSize: \"35px\" }}\n                variant=\"primary\"\n              >\n                üåéInternational.\n              </Button>\n            </Link>\n          </Col>\n          <Col md={6}>\n            <Link to=\"/Indonesia\">\n              <Button\n                className=\"tombol\"\n                style={{ width: \"70%\", height: \"150px\", fontSize: \"35px\" }}\n                variant=\"danger\"\n              >\n                <img src={indoFlag} />\n                Indonesia.\n              </Button>\n            </Link>\n          </Col>\n        </Row>\n        <Row\n          style={{ marginTop: \"10%\" }}\n          className=\"justify-content-md-center aboutus\"\n        >\n          <Link to=\"/Aboutus\" style={{ margin: \"auto\" }}>\n            <h1>About Us.</h1>\n          </Link>\n        </Row>\n        <Switch>\n          <Route exact path=\"/Global\">\n            <Global />\n          </Route>\n          <Route exact path=\"/Indonesia\">\n            <Indonesia />\n          </Route>\n          <Route exact path=\"/Aboutus\">\n            <Aboutus />\n          </Route>\n        </Switch>\n      </Container>\n    );\n  }\n}\n","C:\\Users\\aldii\\OneDrive\\Documents\\Kempus\\semester 3\\F-Introduction to Internet Technology\\Lab\\uas\\uaspti\\src\\Global.js",[],"C:\\Users\\aldii\\OneDrive\\Documents\\Kempus\\semester 3\\F-Introduction to Internet Technology\\Lab\\uas\\uaspti\\src\\Indonesia.js",[],"C:\\Users\\aldii\\OneDrive\\Documents\\Kempus\\semester 3\\F-Introduction to Internet Technology\\Lab\\uas\\uaspti\\src\\Aboutus.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","sever